        <mxCell id="TSlm4UsKuFQV0eDYEWdr-39" value="6" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#007CBD;strokeColor=none;fontColor=#FFFFFF;fontStyle=1;fontSize=22;labelBackgroundColor=none;" parent="TSlm4UsKuFQV0eDYEWdr-1" vertex="1">
